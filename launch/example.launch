<?xml version="1.0"?>
<launch>

    <!-- Command line arguments -->
    <arg name="config_path" default="config/example.yaml" />

    <!-- Load parameter config file -->
    <rosparam command="load" file="$(find hri_audition)/$(arg config_path)" />

    <!-- Launch signal acquisition/feature extraction node -->
    <node name="feature_extraction_node" pkg="hri_audition" type="feature_extraction_node.py" output="screen"/>

</launch>